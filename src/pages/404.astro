---
//Styles
import '../styles/global.css';


//Components
import Layout from '../layouts/Layout.astro';

//Var
const pageTitle = "Page not found";
---

<Layout pageTitle={pageTitle}>
	<div class="container">
		<section>
			<h1>{pageTitle}</h1>
			<p>
				Go home&hellip; <a
					aria-label="Return to home page"
					href="/"
					>Click
				</a>
			</p>
		</section>
	</div>
</Layout>


